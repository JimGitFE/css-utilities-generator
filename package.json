{
  "name": "css-utilities-generator",
  "version": "1.3.2",
  "description": "TypeScript utility package that simplifies working with CSS properties by providing shorthand notations for common CSS class names. Ideal for developers who want to streamline their CSS styling process, this package helps you translate short, intuitive class names into full CSS properties and values, ensuring your code is cleaner and more maintainable.",
  "main": "dist/generator.js",
  "types": "dist/generator.d.ts",
  "scripts": {
    "build": "rimraf -rf ./dist/ && tsc",
    "test": "npm run build && jest",
    "ts-test": "ts-node tests/playground.ts",
    "git-test": "jest",
    "rm-config": "rimraf -rf ./cuconfig.json"
  },
  "bin": {
    "gen-utils": "./generator.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimGitFE/css-utilities-generator.git"
  },
  "keywords": [
    "css",
    "front-end",
    "Utility-First CSS",
    "CSS Utility Framework",
    "Utility Class Generator",
    "CSS Class Shortcuts",
    "Custom CSS Utilities",
    "Dynamic Style Management",
    "On-the-Fly CSS Generation",
    "Utility-Driven Styling",
    "Adaptive CSS",
    "Live CSS Generation",
    "Responsive Utility Classes",
    "Automated CSS Tools",
    "Configurable CSS Utilities",
    "Reusable CSS Classes",
    "Efficient CSS Creation",
    "Inline Style Utilities",
    "Real-Time CSS Updates",
    "Simplified CSS Generation",
    "Utility Class Automation",
    "Dynamic Class Names",
    "styling"
  ],
  "author": "JimGitFE",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JimGitFE/css-utilities-generator/issues"
  },
  "homepage": "https://github.com/JimGitFE/css-utilities-generator#readme",
  "devDependencies": {
    "@babel/generator": "^7.24.5",
    "@babel/parser": "^7.24.5",
    "@babel/traverse": "^7.24.5",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
